# Class: :Bahan_makanan_berpurin (:Bahan_makanan_berpurin)

AnnotationAssertion(rdfs:comment :Bahan_makanan_berpurin "Bahan makanan yang mengandung zat purin. purin adalah komponen dari makanan tertentu yang di metabolisme menjadi asam urat pada tubuh.")
SubClassOf(:Bahan_makanan_berpurin :Bahan_makanan)

# Class: :Bahan_makanan_berpurin_rendah (:Bahan_makanan_berpurin_rendah)

AnnotationAssertion(rdfs:comment :Bahan_makanan_berpurin_rendah "Bahan makanan berpurin rendah adalah bahan makanan yang mengandung jumlah purin rendah antara 0-50mg/100 makanan. contohnya adalah keju, susu dan telur.")
EquivalentClasses(:Bahan_makanan_berpurin_rendah ObjectIntersectionOf(:Bahan_makanan DataSomeValuesFrom(:jumlah_purin DatatypeRestriction(xsd:integer xsd:minInclusive "0"^^xsd:integer xsd:maxExclusive "50"^^xsd:integer))))
SubClassOf(:Bahan_makanan_berpurin_rendah :Bahan_makanan_berpurin)

# Class: :Bahan_makanan_berpurin_sedang (:Bahan_makanan_berpurin_sedang)

AnnotationAssertion(rdfs:comment :Bahan_makanan_berpurin_sedang "Bahan makanan berpurin tinggi adalah bahan makanan yang mengandung jumlah purin sedang yaitu antara 50-150mg/100gram bahan makanan. contohnya adalah kacang-kacangan, aspargus, buncis, jamur, daun singkong, daun pepaya dan kangkung.")
EquivalentClasses(:Bahan_makanan_berpurin_sedang ObjectIntersectionOf(:Bahan_makanan DataSomeValuesFrom(:jumlah_purin DatatypeRestriction(xsd:integer xsd:minInclusive "50"^^xsd:integer xsd:maxInclusive "150"^^xsd:integer))))
SubClassOf(:Bahan_makanan_berpurin_sedang :Bahan_makanan_berpurin)

# Class: :Bahan_makanan_berpurin_tinggi (:Bahan_makanan_berpurin_tinggi)

AnnotationAssertion(rdfs:comment :Bahan_makanan_berpurin_tinggi "Bahan makanan berpurin tinggi adalah bahan makanan yang mengandung jumlah purin tinggi yaitu antara 150-800 mg / 100gram bahan makanan. contohnya adalah hati, ginjal, jantung, paru (semua jeroan), udang, alkohol dan tape (ragi).")
EquivalentClasses(:Bahan_makanan_berpurin_tinggi ObjectIntersectionOf(:Bahan_makanan DataSomeValuesFrom(:jumlah_purin DatatypeRestriction(xsd:integer xsd:minExclusive "150"^^xsd:integer))))
SubClassOf(:Bahan_makanan_berpurin_tinggi :Bahan_makanan_berpurin)

